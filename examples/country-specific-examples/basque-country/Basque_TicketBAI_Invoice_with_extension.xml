<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<Invoice xmlns:cac="urn:pagero:CommonAggregateComponents:1.0"
    xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2"
    xmlns:puf="urn:pagero:ExtensionComponent:1.0"
    xmlns:ext="urn:oasis:names:specification:ubl:schema:xsd:CommonExtensionComponents-2"
    xmlns="urn:pagero:PageroUniversalFormat:Invoice:1.0">
    
    <!-- Values in invoice are examples values only. -->    
    
    <ext:UBLExtensions>
        <!-- Invoice Series if applicable -->
        <ext:UBLExtension>
            <ext:ExtensionURI>urn:pagero:ExtensionComponent:1.0:PageroExtension:InvoiceSeries</ext:ExtensionURI>
            <ext:ExtensionContent>
                <puf:PageroExtension>
                    <puf:InvoiceSeries>
                        <cbc:ID>InvSeries1</cbc:ID>
                    </puf:InvoiceSeries>  
                </puf:PageroExtension>
            </ext:ExtensionContent>
        </ext:UBLExtension>
        <!-- Tax Regime is mandatory in Basque Country -->
        <ext:UBLExtension>
            <ext:ExtensionURI>urn:pagero:ExtensionComponent:1.0:PageroExtension:RestrictedInformation</ext:ExtensionURI>
            <ext:ExtensionContent>
                <puf:PageroExtension>
                    <puf:RestrictedInformation>
                        <puf:Key>SignificantTaxRegime</puf:Key>
                        <puf:Value>01</puf:Value>
                    </puf:RestrictedInformation>
                </puf:PageroExtension>
            </ext:ExtensionContent>
        </ext:UBLExtension>        
    </ext:UBLExtensions>    
    <cbc:CustomizationID>urn:pagero.com:puf:billing:2.0</cbc:CustomizationID>
    <cbc:ProfileID>urn:pagero.com:puf:billing:1.0</cbc:ProfileID>
    <!-- BT-1 Invoice Number -->
    <cbc:ID>123456</cbc:ID>
    <!-- BT-2 Invoice Issue Date  -->
    <cbc:IssueDate>2022-01-01</cbc:IssueDate>
    <!-- BT-9 Due Date -->
    <cbc:DueDate>2022-02-01</cbc:DueDate>
    <!-- BT-3 Invoice Type Code -->
    <cbc:InvoiceTypeCode>380</cbc:InvoiceTypeCode>
    <!-- BT-22 Invoice Note. Mandatory in Basque Country to include. -->
    <cbc:Note>Reason for document. Also header text</cbc:Note>
    <!-- BT-5 Invoice Document Currency -->
    <cbc:DocumentCurrencyCode>EUR</cbc:DocumentCurrencyCode>
    <!-- BG-4 Seller -->
    <cac:AccountingSupplierParty>
        <cac:Party>
            <!-- BT-28 Seller Trading Name -->
            <cac:PartyName>
                <cbc:Name>Seller Trading Name</cbc:Name>
            </cac:PartyName>
            <cac:PostalAddress>
                <!--BT-35 Seller address street-->
                <cbc:StreetName>Street Name</cbc:StreetName>
                <!-- BT-37 Seller address City -->
                <cbc:CityName>City</cbc:CityName>
                <!-- BT-38 Seller address postal code -->
                <cbc:PostalZone>00000</cbc:PostalZone>
                <cac:Country>
                    <!-- BT-40 Seller address country code -->
                    <cbc:IdentificationCode>ES</cbc:IdentificationCode>
                </cac:Country>
            </cac:PostalAddress>
            <cac:PartyTaxScheme>
                <!-- BT-31 Seller Vat Number -->
                <cbc:CompanyID>ESA123456789</cbc:CompanyID>
                <cac:TaxScheme>
                    <cbc:ID>VAT</cbc:ID>
                </cac:TaxScheme>
            </cac:PartyTaxScheme>
            <cac:PartyLegalEntity>
                <!-- BT-27 Seller Registration Name -->
                <cbc:RegistrationName>Supplier Registration Name</cbc:RegistrationName>
                <!-- BT-30 Seller Legal Registration Identifier-->
                <cbc:CompanyID>A12345678</cbc:CompanyID> 
            </cac:PartyLegalEntity>
        </cac:Party>
    </cac:AccountingSupplierParty>
    <!-- BG-7 Buyer -->
    <cac:AccountingCustomerParty>
        <!-- Customer number, not a requirement in Basque Country but can be useful to route document to correct recipient in pageroonline -->
        <cbc:SupplierAssignedAccountID>Customer Number</cbc:SupplierAssignedAccountID>
        <cac:Party>
            <!-- BT-45 Buyer Trading Name -->
            <cac:PartyName>
                <cbc:Name>Buyer Trading Name</cbc:Name>
            </cac:PartyName>
            <cac:PostalAddress>
                <!-- BT-50 Buyer address street-->
                <cbc:StreetName>Street Name</cbc:StreetName>
                <!-- BT-52 Buyer address City -->
                <cbc:CityName>City</cbc:CityName>
                <!-- BT-53 Buyer address postal code -->
                <cbc:PostalZone>00000</cbc:PostalZone>
                <cac:Country>
                    <!-- BT-55 Buyer address country code -->
                    <cbc:IdentificationCode>ES</cbc:IdentificationCode>
                </cac:Country>
            </cac:PostalAddress>
            <cac:PartyTaxScheme>
                <!-- BT-48 Buyer Vat Number if applicable -->
                <cbc:CompanyID>ESA987654321</cbc:CompanyID>
                <cac:TaxScheme>
                    <cbc:ID>VAT</cbc:ID>
                </cac:TaxScheme>
            </cac:PartyTaxScheme>
            <cac:PartyLegalEntity>
                <!-- BT-44 Buyer Name -->
                <cbc:RegistrationName>Buyer Name</cbc:RegistrationName>
                <!-- BT-47 Buyer Legal Registration Identifier-->
                <cbc:CompanyID>A87654321</cbc:CompanyID> 
            </cac:PartyLegalEntity>
        </cac:Party>
    </cac:AccountingCustomerParty>
    <cac:Delivery>
        <!-- BT-72 Actual Delivery Date-->
        <cbc:ActualDeliveryDate>2022-01-01</cbc:ActualDeliveryDate>
    </cac:Delivery>
    <cac:PaymentMeans>
        <!-- BT-81 Payment means code. -->
        <cbc:PaymentMeansCode>30</cbc:PaymentMeansCode>
        <!-- BT-83 Remittance information -->
        <cbc:PaymentID>123456/2022</cbc:PaymentID>
        <cac:PayeeFinancialAccount>
            <!-- BT-84 A unique identifier of the financial payment account -->
            <cbc:ID>IBAN32423940</cbc:ID>
            <cac:FinancialInstitutionBranch>
                <!-- BT-86 An identifier for the payment service provider where a payment account is located. Such as a BIC or a national clearing code where required -->
                <cbc:ID>BIC324098</cbc:ID>
            </cac:FinancialInstitutionBranch>
        </cac:PayeeFinancialAccount>
    </cac:PaymentMeans>
    
    <!-- BG-20 AllowanceCharge conditionally mandatory if applicable -->
    <cac:AllowanceCharge>
        <!-- ChargeIndicator. false to indicate discount-->
        <cbc:ChargeIndicator>false</cbc:ChargeIndicator>
        <!-- BT-92 Document level allowance amount without VAT. Currency in attribute currencyID (BT-5) -->
        <cbc:Amount currencyID="EUR">5.00</cbc:Amount>
        <cac:TaxCategory>
            <!-- BT-95 Document level allowanec VAT category code. -->
            <cbc:ID>E</cbc:ID>
            <!-- BT-96 The VAT rate, represented as percentage that applies to the document level allowance. -->
            <cbc:Percent>0</cbc:Percent>
            <cac:TaxScheme>
                <cbc:ID>VAT</cbc:ID>
            </cac:TaxScheme>
        </cac:TaxCategory>
    </cac:AllowanceCharge>
    <cac:TaxTotal>
        <!-- BT-110 Invoice total VAT amount. Currency in attribute currencyID (BT-5) -->
        <cbc:TaxAmount currencyID="EUR">15.00</cbc:TaxAmount>
        <cac:TaxSubtotal>
            <!-- BT-116 Sum of all taxable amounts subject to a specific VAT category code and VAT category rate. Currency in attribute currencyID (BT-5) -->
            <cbc:TaxableAmount currencyID="EUR">100.00</cbc:TaxableAmount>
            <!--  BT-117. The total VAT amount for a given VAT category. Currency in attribute currencyID (BT-5) -->
            <cbc:TaxAmount currencyID="EUR">15.00</cbc:TaxAmount>
            <cac:TaxCategory>
                <!-- BT-118 VAT category code. -->
                <cbc:ID>S</cbc:ID>
                <!-- BT-119 VAT category rate-->
                <cbc:Percent>15.00</cbc:Percent>
                <cac:TaxScheme>
                    <cbc:ID>VAT</cbc:ID>
                </cac:TaxScheme>
            </cac:TaxCategory>
        </cac:TaxSubtotal>
        <cac:TaxSubtotal>
            <!-- BT-116 Sum of all taxable amounts subject to a specific VAT category code and VAT category rate. Currency in attribute currencyID (BT-5) -->
            <cbc:TaxableAmount currencyID="EUR">95.00</cbc:TaxableAmount>
            <!--  BT-117. The total VAT amount for a given VAT category. Currency in attribute currencyID (BT-5) -->
            <cbc:TaxAmount currencyID="EUR">0.00</cbc:TaxAmount>
            <cac:TaxCategory>
                <!-- BT-118 VAT category code. -->
                <cbc:ID>E</cbc:ID>
                <!-- BT-119 VAT category rate-->
                <cbc:Percent>0</cbc:Percent>
                <!-- BT-121 VAT exemption reason code -->
                <cbc:TaxExemptionReasonCode>Tax exemption reason code</cbc:TaxExemptionReasonCode>
                <!-- BT-120 VAT exemption reason text. -->
                <cbc:TaxExemptionReason>Tax exemption reason</cbc:TaxExemptionReason>
                <cac:TaxScheme>
                    <cbc:ID>VAT</cbc:ID>
                </cac:TaxScheme>
            </cac:TaxCategory>
        </cac:TaxSubtotal>
    </cac:TaxTotal>
    <cac:LegalMonetaryTotal>
        <!-- BT-106 Sum of all Invoice line net amounts in the Invoice  without VAT. Currency in attribute currencyID (BT-5)-->
        <cbc:LineExtensionAmount currencyID="EUR">200.00</cbc:LineExtensionAmount>
        <!-- BT-109 Invoice total amount without VAT. Currency in attribute currencyID (BT-5)-->
        <cbc:TaxExclusiveAmount currencyID="EUR">195.00</cbc:TaxExclusiveAmount>
        <!-- BT-112 Invoice total amount with VAT. Currency in attribute currencyID (BT-5) -->
        <cbc:TaxInclusiveAmount currencyID="EUR">210.00</cbc:TaxInclusiveAmount>
        <!-- BT-107 Sum of all allowances on document level. Currency in attribute currencyID (BT-5)-->
        <cbc:AllowanceTotalAmount currencyID="EUR">5.00</cbc:AllowanceTotalAmount>
        <!-- BT-115 Amount due for payment. Currency in attribute currencyID (BT-5) -->
        <cbc:PayableAmount currencyID="EUR">210.00</cbc:PayableAmount>
    </cac:LegalMonetaryTotal>
    <cac:InvoiceLine>
        <!-- BT-126 Line ID -->
        <cbc:ID>1</cbc:ID>
        <!-- BT-129 Invoiced Quantity. BT-130 unit of measure in attribute unitCode -->
        <cbc:InvoicedQuantity unitCode="EA">1</cbc:InvoicedQuantity>
        <!-- BT-131 Line extension amount. Line net amount. Currency in attribute currencyID (BT-5) -->
        <cbc:LineExtensionAmount currencyID="EUR">100.00</cbc:LineExtensionAmount>
        <!-- Conditionally Mandatory if discount is used on line level -->
        <cac:AllowanceCharge>
            <!-- ChargeIndicator must be false to indicate discount-->
            <cbc:ChargeIndicator>false</cbc:ChargeIndicator>
            <!-- BT-136 Discount amount without tax. Currency in attribute currencyID (BT-5) -->
            <cbc:Amount currencyID="EUR">10</cbc:Amount>
        </cac:AllowanceCharge>
        <cac:Item>
            <!-- BT-153 Article/Item Description -->
            <cbc:Name>Article 1</cbc:Name>
            <cac:ClassifiedTaxCategory>
                <!-- BT-151 Invoiced item VAT category code. -->
                <cbc:ID schemeID="UNCL5305">S</cbc:ID>
                <!-- BT-152 Invoiced item VAT rate -->
                <cbc:Percent>15</cbc:Percent>
                <cac:TaxScheme>
                    <cbc:ID>VAT</cbc:ID>
                </cac:TaxScheme>
            </cac:ClassifiedTaxCategory>
        </cac:Item>
        <cac:Price>
            <!-- BT-146 Item net price. Currency in attribute currencyID (BT-5) -->
            <cbc:PriceAmount currencyID="EUR">110.00</cbc:PriceAmount>
        </cac:Price>
    </cac:InvoiceLine>
    <cac:InvoiceLine>
        <!-- BT-126 Line ID -->
        <cbc:ID>2</cbc:ID>
        <!-- BT-129 Invoiced Quantity. BT-130 unit of measure in attribute unitCode -->
        <cbc:InvoicedQuantity unitCode="EA">1</cbc:InvoicedQuantity>
        <!-- BT-131 Line extension amount. Line net amount. Currency in attribute currencyID (BT-5) -->
        <cbc:LineExtensionAmount currencyID="EUR">100.00</cbc:LineExtensionAmount>
        <cac:Item>
            <!-- BT-153 Article/Item Description -->
            <cbc:Name>Article 2</cbc:Name>
            <cac:ClassifiedTaxCategory>
                <!-- BT-151 Invoiced item VAT category code. -->
                <cbc:ID schemeID="UNCL5305">E</cbc:ID>
                <!-- BT-152 Invoiced item VAT rate -->
                <cbc:Percent>0</cbc:Percent>
                <cac:TaxScheme>
                    <cbc:ID>VAT</cbc:ID>
                </cac:TaxScheme>
            </cac:ClassifiedTaxCategory>
        </cac:Item>
        <cac:Price>
            <!-- BT-146 Item net price. Currency in attribute currencyID (BT-5) -->
            <cbc:PriceAmount currencyID="EUR">100.00</cbc:PriceAmount>
        </cac:Price>
    </cac:InvoiceLine>
</Invoice>