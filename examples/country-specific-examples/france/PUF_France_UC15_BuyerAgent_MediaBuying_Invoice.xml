            </cac:AdditionalItemProperty>
        </cac:Item>

        <!-- BG-29: Price details -->
        <cac:Price>
            <!-- BT-146: Item net price -->
            <cbc:PriceAmount currencyID="EUR">15000.00</cbc:PriceAmount>
            <!-- BT-149: Item price base quantity -->
            <cbc:BaseQuantity unitCode="C62">1</cbc:BaseQuantity>
