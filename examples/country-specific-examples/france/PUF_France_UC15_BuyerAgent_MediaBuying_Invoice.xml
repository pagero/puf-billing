        <!-- BT-106: Sum of Invoice line net amount -->
        <cbc:LineExtensionAmount currencyID="EUR">36000.00</cbc:LineExtensionAmount>
        <!-- BT-109: Invoice total amount without VAT -->
        <cbc:TaxExclusiveAmount currencyID="EUR">36000.00</cbc:TaxExclusiveAmount>
        <!-- BT-112: Invoice total amount with VAT -->
        <cbc:TaxInclusiveAmount currencyID="EUR">43200.00</cbc:TaxInclusiveAmount>
        <!-- BT-115: Amount due for payment (paid by BUYER AGENT) -->
        <cbc:PayableAmount currencyID="EUR">43200.00</cbc:PayableAmount>
    </cac:LegalMonetaryTotal>

    <!-- BG-25: INVOICE LINE 1 - Display Banner Advertising -->
    <cac:InvoiceLine>
        <!-- BT-126: Invoice line identifier -->
        <cbc:ID>1</cbc:ID>

        <!-- BT-129: Invoiced quantity (Campaign delivery impressions) -->
        <cbc:InvoicedQuantity unitCode="C62">1</cbc:InvoicedQuantity>

        <!-- BT-131: Invoice line net amount -->
        <cbc:LineExtensionAmount currencyID="EUR">15000.00</cbc:LineExtensionAmount>

        <!-- BT-72: Actual delivery date (Campaign period) -->
        <cac:InvoicePeriod>
            <cbc:StartDate>2026-01-15</cbc:StartDate>
            <cbc:EndDate>2026-01-31</cbc:EndDate>
        </cac:InvoicePeriod>

        <!-- BG-31: Item information -->
        <cac:Item>
            <!-- BT-154: Item description -->
            <cbc:Description>Campagne publicitaire display bannières premium - Lancement gamme soins anti-âge "Éternelle Jeunesse": diffusion bannières 300x250 et 728x90 sur sites éditoriaux premium beauté/lifestyle (Marie Claire, Elle, Cosmopolitan, Vogue digital), audience qualifiée femmes CSP+ 30-55 ans, garantie visibilité 100% above the fold, fréquence capping 3 impressions/jour/utilisateur, ciblage géographique Île-de-France + grandes métropoles, période du 15 au 31 janvier 2026, objectif 2,5 millions d'impressions livrées, CTR garanti ≥0.8%, tracking pixels conversion installés, rapport hebdomadaire de performance fourni</cbc:Description>

            <!-- BT-153: Item name -->
            <cbc:Name>Publicité Display Bannières Premium - Campagne "Éternelle Jeunesse" Q1 2026</cbc:Name>

