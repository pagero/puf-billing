                <!-- BT-36: Seller address line 2 -->
                <cbc:AdditionalStreetName>Zone Artisanale du Bois Joli</cbc:AdditionalStreetName>
                <!-- BT-37: Seller city -->
                <cbc:CityName>Villeurbanne</cbc:CityName>
                <!-- BT-38: Seller post code -->
                <cbc:PostalZone>69100</cbc:PostalZone>
                <!-- BT-40: Seller country code -->
                <cac:Country>
                    <cbc:IdentificationCode>FR</cbc:IdentificationCode>
                </cac:Country>
            </cac:PostalAddress>

            <!-- BT-31: Seller VAT identifier -->
            <cac:PartyTaxScheme>
                <cbc:CompanyID>FR55555444333</cbc:CompanyID>
                <cac:TaxScheme>
                    <cbc:ID>VAT</cbc:ID>
                </cac:TaxScheme>
            </cac:PartyTaxScheme>

            <!-- BT-27: Seller legal name (Actual supplier - Artisan furniture maker) -->
            <cac:PartyLegalEntity>
                <cbc:RegistrationName>Ébénisterie Artisanale Dubois EURL</cbc:RegistrationName>
                <!-- BT-30: Seller legal registration identifier (SIREN) -->
                <cbc:CompanyID schemeID="0002">555444333</cbc:CompanyID>
            </cac:PartyLegalEntity>

            <!-- BG-6: Seller contact (Owner/Master Craftsman) -->
            <cac:Contact>
                <!-- BT-41: Seller contact point -->
                <cbc:Name>Jean-Pierre Dubois - Maître Ébéniste</cbc:Name>
                <!-- BT-42: Seller contact telephone -->
                <cbc:Telephone>+33 4 72 34 56 78</cbc:Telephone>
                <!-- BT-43: Seller contact email -->
                <cbc:ElectronicMail>jp.dubois@ebenisterie-dubois.fr</cbc:ElectronicMail>
            </cac:Contact>

            <!-- Billing Agent (Invoicer) - Third Party Creating Invoice on Behalf of SELLER
                 Under French billing mandate (Mandataire Facturant), the agent creates
                 invoices in the SELLER's name. The agent is identified here as the service
                 provider party with role code "II" (Invoicer - UNTDID 3035).
                 
                 The billing agent handles:
                 - Invoice creation and transmission
                 - Lifecycle status management
                 - Platform coordination
                 
                 The SELLER retains:
                 - Ownership of receivable
                 - Right to collect payment
                 - Legal responsibility for sale -->
            <cac:ServiceProviderParty>
                <cac:Party>
                    <!-- Billing Agent Role Code: II (Invoicer) -->
                    <ext:UBLExtensions>
                        <ext:UBLExtension>
                            <ext:ExtensionURI>urn:pagero:ExtensionComponent:1.0:PageroExtension:PartyExtension</ext:ExtensionURI>
                            <ext:ExtensionContent>
                                <puf:PageroExtension>
                                    <puf:PartyExtension>
                                        <puf:RoleCode>
                                            <cbc:ID>II</cbc:ID>
                                        </puf:RoleCode>
                                    </puf:PartyExtension>
                                </puf:PageroExtension>
                            </ext:ExtensionContent>
                        </ext:UBLExtension>
                    </ext:UBLExtensions>

                    <!-- Billing Agent Electronic Address (Status Reception) -->
                    <cbc:EndpointID schemeID="0002">88877766655544</cbc:EndpointID>

                    <!-- Billing Agent Trading Name -->
                    <cac:PartyName>
                        <cbc:Name>ComptaExpert Services</cbc:Name>
                    </cac:PartyName>

                    <!-- Billing Agent Postal Address -->
                    <cac:PostalAddress>
                        <cbc:StreetName>12 Rue de la Comptabilité</cbc:StreetName>
                        <cbc:CityName>Lyon</cbc:CityName>
                        <cbc:PostalZone>69002</cbc:PostalZone>
                        <cac:Country>
                            <cbc:IdentificationCode>FR</cbc:IdentificationCode>
                        </cac:Country>
                    </cac:PostalAddress>

                    <!-- Billing Agent VAT Identifier -->
                    <cac:PartyTaxScheme>
                        <cbc:CompanyID>FR88888777666</cbc:CompanyID>
                        <cac:TaxScheme>
                            <cbc:ID>VAT</cbc:ID>
                        </cac:TaxScheme>
                    </cac:PartyTaxScheme>

                    <!-- Billing Agent Legal Entity (Company details for billing administration) -->
                    <cac:PartyLegalEntity>
                        <cbc:RegistrationName>ComptaExpert Services SARL</cbc:RegistrationName>
                        <!-- Billing Agent SIREN -->
                        <cbc:CompanyID schemeID="0002">888777666</cbc:CompanyID>
                    </cac:PartyLegalEntity>

                    <!-- Billing Agent Contact -->
                    <cac:Contact>
                        <cbc:Name>Marie Dupont - Responsable Facturation</cbc:Name>
                        <cbc:Telephone>+33 4 78 90 12 34</cbc:Telephone>
                        <cbc:ElectronicMail>facturation@comptaexpert-services.fr</cbc:ElectronicMail>
                    </cac:Contact>
                </cac:Party>
            </cac:ServiceProviderParty>
        </cac:Party>
    </cac:AccountingSupplierParty>

    <!-- BG-7: BUYER (Accounting Customer Party) - Corporate Office Buyer -->
    <cac:AccountingCustomerParty>
        <cac:Party>
            <!-- BT-49: Buyer electronic address -->
            <cbc:EndpointID schemeID="0002">11122233344455</cbc:EndpointID>

            <!-- BT-45: Buyer trading name -->
            <cac:PartyName>
                <cbc:Name>Solutions Bureau Corporate</cbc:Name>
            </cac:PartyName>

            <!-- BG-8: Buyer postal address -->
            <cac:PostalAddress>
                <!-- BT-50: Buyer address line 1 -->
                <cbc:StreetName>25 Boulevard des Entreprises</cbc:StreetName>
                <!-- BT-52: Buyer city -->
                <cbc:CityName>Lyon</cbc:CityName>
                <!-- BT-53: Buyer post code -->
                <cbc:PostalZone>69003</cbc:PostalZone>
                <!-- BT-55: Buyer country code -->
                <cac:Country>
                    <cbc:IdentificationCode>FR</cbc:IdentificationCode>
                </cac:Country>
            </cac:PostalAddress>

            <!-- BT-48: Buyer VAT identifier -->
            <cac:PartyTaxScheme>
                <cbc:CompanyID>FR11111222333</cbc:CompanyID>
                <cac:TaxScheme>
                    <cbc:ID>VAT</cbc:ID>
                </cac:TaxScheme>
            </cac:PartyTaxScheme>

            <!-- BT-44: Buyer legal name -->
            <cac:PartyLegalEntity>
                <cbc:RegistrationName>Solutions Bureau Corporate SAS</cbc:RegistrationName>
                <!-- BT-47: Buyer legal registration identifier (SIREN) -->
                <cbc:CompanyID schemeID="0002">111222333</cbc:CompanyID>
            </cac:PartyLegalEntity>

            <!-- BG-9: Buyer contact -->
            <cac:Contact>
                <!-- BT-56: Buyer contact point -->
                <cbc:Name>Sylvie Moreau - Responsable Achats</cbc:Name>
                <!-- BT-57: Buyer contact telephone -->
                <cbc:Telephone>+33 4 78 12 34 56</cbc:Telephone>
                <!-- BT-58: Buyer contact email -->
                <cbc:ElectronicMail>s.moreau@sbc-lyon.fr</cbc:ElectronicMail>
            </cac:Contact>
        </cac:Party>
    </cac:AccountingCustomerParty>

    <!-- BG-17: Credit transfer (Payment means) -->
    <cac:PaymentMeans>
        <!-- BT-81: Payment means type code (31 = credit transfer) -->
        <cbc:PaymentMeansCode>31</cbc:PaymentMeansCode>

        <!-- BT-83: Payment reference -->
        <cbc:PaymentID>EAD-2026-0234</cbc:PaymentID>

        <!-- BG-19: Credit transfer - SELLER's IBAN
             CRITICAL: Payment MUST be made to SELLER (artisan), NOT to Billing Agent.
             The agent creates invoice but has NO right to collect payment. -->
        <cac:PayeeFinancialAccount>
            <!-- BT-84: Payment account identifier (SELLER's IBAN) -->
            <cbc:ID>FR7655544433322211112222333344</cbc:ID>
            <!-- BT-85: Payment account name -->
            <cbc:Name>Ébénisterie Artisanale Dubois EURL</cbc:Name>
        </cac:PayeeFinancialAccount>
    </cac:PaymentMeans>

    <!-- BG-20: Payment terms -->
    <cac:PaymentTerms>
        <!-- BT-20: Payment terms (free text) -->
        <cbc:Note>Paiement à 30 jours fin de mois par virement bancaire. En cas de retard, pénalités au taux de 3 fois le taux d'intérêt légal en vigueur, plus indemnité forfaitaire de recouvrement de 40 EUR (articles L. 441-6 et D. 441-5 du Code de commerce). IMPORTANT - FACTURATION MANDATÉE: Cette facture a été établie par le mandataire facturant ComptaExpert Services SARL pour le compte du VENDEUR Ébénisterie Artisanale Dubois EURL. Le règlement doit être effectué directement au VENDEUR sur le compte bancaire indiqué (IBAN FR76 5554 4433 3222 1111 2222 3333 44). Le mandataire facturant n'est pas habilité à recevoir le paiement et n'intervient que pour la création et la transmission de la facture conformément au contrat de mandat.</cbc:Note>
    </cac:PaymentTerms>

    <!-- BG-23: VAT breakdown -->
    <cac:TaxTotal>
        <!-- BT-110: Invoice total VAT amount -->
        <cbc:TaxAmount currencyID="EUR">1200.00</cbc:TaxAmount>

        <!-- BG-23: VAT category taxable amount and VAT -->
        <cac:TaxSubtotal>
            <!-- BT-116: VAT category taxable amount -->
            <cbc:TaxableAmount currencyID="EUR">7200.00</cbc:TaxableAmount>
            <!-- BT-117: VAT category tax amount -->
            <cbc:TaxAmount currencyID="EUR">1200.00</cbc:TaxAmount>
            <!-- BT-118: VAT category code (S = Standard rate) -->
            <cac:TaxCategory>
                <cbc:ID>S</cbc:ID>
                <!-- BT-119: VAT rate (20% standard rate) -->
                <cbc:Percent>20.00</cbc:Percent>
                <cac:TaxScheme>
                    <cbc:ID>VAT</cbc:ID>
                </cac:TaxScheme>
            </cac:TaxCategory>
        </cac:TaxSubtotal>
    </cac:TaxTotal>

    <!-- BG-22: Document totals -->
    <cac:LegalMonetaryTotal>
        <!-- BT-106: Sum of Invoice line net amount -->
        <cbc:LineExtensionAmount currencyID="EUR">7200.00</cbc:LineExtensionAmount>
        <!-- BT-109: Invoice total amount without VAT -->
        <cbc:TaxExclusiveAmount currencyID="EUR">7200.00</cbc:TaxExclusiveAmount>
        <!-- BT-112: Invoice total amount with VAT -->
        <cbc:TaxInclusiveAmount currencyID="EUR">8400.00</cbc:TaxInclusiveAmount>
        <!-- BT-115: Amount due for payment -->
        <cbc:PayableAmount currencyID="EUR">8400.00</cbc:PayableAmount>
    </cac:LegalMonetaryTotal>

    <!-- BG-25: Invoice lines -->

    <!-- Line 1: Custom executive desk (solid oak) -->
    <cac:InvoiceLine>
        <!-- BT-126: Invoice line identifier -->
        <cbc:ID>1</cbc:ID>

        <!-- BT-129: Invoiced quantity -->
        <cbc:InvoicedQuantity unitCode="PCE">1</cbc:InvoicedQuantity>

        <!-- BT-131: Invoice line net amount -->
        <cbc:LineExtensionAmount currencyID="EUR">4500.00</cbc:LineExtensionAmount>

        <!-- BG-31: Item information -->
        <cac:Item>
