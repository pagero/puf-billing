        <cac:Price>
            <!-- BT-146: Item net price -->
            <cbc:PriceAmount currencyID="EUR">1200.00</cbc:PriceAmount>
            <!-- BT-149: Base quantity -->
            <cbc:BaseQuantity unitCode="PCE">1</cbc:BaseQuantity>
        </cac:Price>
    </cac:InvoiceLine>

    <!-- Line 3: Delivery and assembly service -->
    <cac:InvoiceLine>
        <!-- BT-126: Invoice line identifier -->
        <cbc:ID>3</cbc:ID>

        <!-- BT-129: Invoiced quantity -->
        <cbc:InvoicedQuantity unitCode="HUR">4</cbc:InvoicedQuantity>

        <!-- BT-131: Invoice line net amount -->
        <cbc:LineExtensionAmount currencyID="EUR">300.00</cbc:LineExtensionAmount>

        <!-- BG-31: Item information -->
        <cac:Item>
            <!-- BT-154: Item description -->
            <cbc:Description>Service de livraison et d'installation professionnelle du mobilier sur site client. Inclut: transport sécurisé avec camion adapté (protection meubles), portage jusqu'au bureau (3ème étage), montage et assemblage complet du bureau et des fauteuils, réglages ergonomiques personnalisés, nettoyage et évacuation des emballages. Intervention artisan qualifié. Durée estimée: 4 heures.</cbc:Description>
            <!-- BT-153: Item name -->
            <cbc:Name>Livraison et montage sur site</cbc:Name>

