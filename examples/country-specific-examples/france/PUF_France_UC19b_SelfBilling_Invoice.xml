        <!-- BT-98: Document level allowance reason -->
        <cbc:AllowanceChargeReason>Consigne emballages retournables (cagettes plastique)</cbc:AllowanceChargeReason>
        <!-- BT-92: Document level allowance amount -->
        <cbc:Amount currencyID="EUR">192.00</cbc:Amount>
        <!-- BT-95: Document level allowance VAT category code -->
        <cac:TaxCategory>
            <cbc:ID>S</cbc:ID>
            <cbc:Percent>20.00</cbc:Percent>
            <cac:TaxScheme>
                <cbc:ID>VAT</cbc:ID>
            </cac:TaxScheme>
        </cac:TaxCategory>
    </cac:AllowanceCharge>

