            <cac:Address>
                <cbc:StreetName>Zone Logistique Nord - Entrepôt 3</cbc:StreetName>
                <cbc:CityName>Nantes</cbc:CityName>
                <cbc:PostalZone>44300</cbc:PostalZone>
                <cac:Country>
                    <cbc:IdentificationCode>FR</cbc:IdentificationCode>
