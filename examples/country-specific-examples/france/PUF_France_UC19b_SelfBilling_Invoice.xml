                 Routes to BUYER's PA-E (emission platform).
                 BUYER creates and emits invoice from here. -->
            <cbc:EndpointID schemeID="0002">99988877766655</cbc:EndpointID>

            <!-- BT-45: Buyer trading name -->
            <cac:PartyName>
                <cbc:Name>SuperMarchés Bio France</cbc:Name>
            </cac:PartyName>

            <!-- BG-8: Buyer postal address -->
            <cac:PostalAddress>
                <!-- BT-50: Buyer address line 1 -->
                <cbc:StreetName>50 Avenue du Commerce Équitable</cbc:StreetName>
                <!-- BT-52: Buyer city -->
                <cbc:CityName>Nantes</cbc:CityName>
                <!-- BT-53: Buyer post code -->
                <cbc:PostalZone>44000</cbc:PostalZone>
                <!-- BT-55: Buyer country code -->
                <cac:Country>
                    <cbc:IdentificationCode>FR</cbc:IdentificationCode>
                </cac:Country>
            </cac:PostalAddress>

            <!-- BT-48: Buyer VAT identifier -->
            <cac:PartyTaxScheme>
                <cbc:CompanyID>FR99999888777</cbc:CompanyID>
                <cac:TaxScheme>
                    <cbc:ID>VAT</cbc:ID>
                </cac:TaxScheme>
            </cac:PartyTaxScheme>

            <!-- BT-44: Buyer legal name (Creates invoice for their purchases) -->
            <cac:PartyLegalEntity>
                <cbc:RegistrationName>SuperMarchés Bio France SAS</cbc:RegistrationName>
                <!-- BT-47: Buyer legal registration identifier (SIREN) -->
                <cbc:CompanyID schemeID="0002">999888777</cbc:CompanyID>
            </cac:PartyLegalEntity>

            <!-- BG-9: Buyer contact (Purchasing Manager) -->
            <cac:Contact>
                <!-- BT-56: Buyer contact point -->
                <cbc:Name>Martine Dubois - Responsable Achats Frais</cbc:Name>
                <!-- BT-57: Buyer contact telephone -->
                <cbc:Telephone>+33 2 40 11 22 33</cbc:Telephone>
                <!-- BT-58: Buyer contact email -->
                <cbc:ElectronicMail>m.dubois@supermarches-bio.fr</cbc:ElectronicMail>
            </cac:Contact>
        </cac:Party>
    </cac:AccountingCustomerParty>

    <!-- BG-13: Delivery information -->
    <cac:Delivery>
        <!-- BT-72: Actual delivery date -->
        <cbc:ActualDeliveryDate>2026-01-30</cbc:ActualDeliveryDate>

        <!-- BG-15: Deliver to address (Supermarket warehouse) -->
