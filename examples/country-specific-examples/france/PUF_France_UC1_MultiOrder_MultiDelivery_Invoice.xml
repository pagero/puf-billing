            <!-- EXT-FR-FE-BG-10: Delivery location details -->
            <cac:DeliveryLocation>
                <!-- EXT-FR-FE-146: Location identifier (GLN) -->
                <cbc:ID schemeID="0088">3597890123456</cbc:ID>
                <!-- EXT-FR-FE-149: Location name -->
                <cbc:Name>Entrepôt Central Lyon</cbc:Name>
                <!-- EXT-FR-FE-150: Delivery address at line level -->
                <cac:Address>
                    <!-- EXT-FR-FE-151: Street name -->
                    <cbc:StreetName>45 Rue de la Logistique</cbc:StreetName>
                    <cbc:AdditionalStreetName>Zone Industrielle Nord</cbc:AdditionalStreetName>
                    <!-- EXT-FR-FE-154: City -->
                    <cbc:CityName>Lyon</cbc:CityName>
                    <!-- EXT-FR-FE-155: Postal code -->
                    <cbc:PostalZone>69007</cbc:PostalZone>
                    <!-- EXT-FR-FE-156: Country subdivision (optional) -->
                    <cbc:CountrySubentity>Auvergne-Rhône-Alpes</cbc:CountrySubentity>
                    <cac:Country>
                        <!-- EXT-FR-FE-157: Country code -->
                        <cbc:IdentificationCode>FR</cbc:IdentificationCode>
                        <cbc:Name>France</cbc:Name>
                    </cac:Country>
                </cac:Address>
            </cac:DeliveryLocation>
        </cac:Delivery>

        <!-- BG-25 Invoice line -->
        <cac:Item>
            <!-- BT-154 Item description -->
            <cbc:Description>Café 100% Arabica torréfaction artisanale, origine Colombie</cbc:Description>
            <!-- BT-153 Item name -->
            <cbc:Name>Café Arabica Premium - Grains 1kg</cbc:Name>
            <!-- BG-32 Item classification -->
            <cac:CommodityClassification>
                <!-- BT-158 Item classification identifier -->
                <cbc:ItemClassificationCode listID="STI">09012100</cbc:ItemClassificationCode>
            </cac:CommodityClassification>
            <!-- BG-30 Line VAT information -->
            <cac:ClassifiedTaxCategory>
                <!-- BT-151 Item VAT category code -->
                <cbc:ID>S</cbc:ID>
                <!-- BT-152 Item VAT rate -->
                <cbc:Percent>20.00</cbc:Percent>
                <cac:TaxScheme>
                    <cbc:ID>VAT</cbc:ID>
                </cac:TaxScheme>
            </cac:ClassifiedTaxCategory>
            <!-- BG-31 Item attributes -->
            <cac:AdditionalItemProperty>
                <cbc:Name>Origine</cbc:Name>
                <cbc:Value>Colombie</cbc:Value>
            </cac:AdditionalItemProperty>
            <cac:AdditionalItemProperty>
                <cbc:Name>Certification</cbc:Name>
                <cbc:Value>Bio AB</cbc:Value>
            </cac:AdditionalItemProperty>
        </cac:Item>
        <!-- BG-29 Price details -->
        <cac:Price>
            <!-- BT-146 Item net price -->
            <cbc:PriceAmount currencyID="EUR">10.00</cbc:PriceAmount>
            <!-- BT-149 Base quantity -->
            <cbc:BaseQuantity unitCode="EA">1</cbc:BaseQuantity>
        </cac:Price>
    </cac:InvoiceLine>

    <!-- ========================================
         INVOICE LINE 2: Order PO-2026-002, Delivery to Paris Centre Store
         ======================================== -->
    <cac:InvoiceLine>
        <!-- BT-126 Invoice line identifier -->
        <cbc:ID>2</cbc:ID>
        <!-- BT-132 Invoice line note -->
        <cbc:Note>Livraison magasin Paris Centre</cbc:Note>
        <!-- BT-129 Invoiced quantity -->
        <cbc:InvoicedQuantity unitCode="EA">50</cbc:InvoicedQuantity>
        <!-- BT-131 Invoice line net amount -->
        <cbc:LineExtensionAmount currencyID="EUR">1250.00</cbc:LineExtensionAmount>

        <!-- EXT-FR-FE-135: Order reference at line level (different order from line 1) -->
        <cac:OrderLineReference>
            <cbc:LineID>8</cbc:LineID>
            <cac:OrderReference>
                <!-- This line references a different purchase order -->
                <cbc:ID>PO-2026-002</cbc:ID>
                <cbc:IssueDate>2026-01-08</cbc:IssueDate>
            </cac:OrderReference>
        </cac:OrderLineReference>

        <!-- EXT-FR-FE-BG-07: Despatch advice reference at line level -->
        <cac:DespatchLineReference>
            <!-- EXT-FR-FE-141: Despatch advice line reference -->
            <cbc:LineID>1</cbc:LineID>
            <cac:DocumentReference>
                <!-- EXT-FR-FE-140: Different despatch advice for this delivery -->
                <cbc:ID>DN-2026-0046</cbc:ID>
                <cbc:IssueDate>2026-01-11</cbc:IssueDate>
            </cac:DocumentReference>
