                <!-- BT-151 Item VAT category code -->
                <cbc:ID>S</cbc:ID>
                <!-- BT-152 Item VAT rate -->
                <cbc:Percent>20.00</cbc:Percent>
                <cac:TaxScheme>
                    <cbc:ID>VAT</cbc:ID>
                </cac:TaxScheme>
            </cac:ClassifiedTaxCategory>
            <!-- BG-31 Item attributes -->
            <cac:AdditionalItemProperty>
                <cbc:Name>Type</cbc:Name>
                <cbc:Value>Thé vert</cbc:Value>
            </cac:AdditionalItemProperty>
            <cac:AdditionalItemProperty>
                <cbc:Name>Conditionnement</cbc:Name>
                <cbc:Value>100 sachets</cbc:Value>
            </cac:AdditionalItemProperty>
        </cac:Item>
        <cac:Price>
            <!-- BT-146 Item net price -->
            <cbc:PriceAmount currencyID="EUR">25.00</cbc:PriceAmount>
            <!-- BT-149 Base quantity -->
            <cbc:BaseQuantity unitCode="EA">1</cbc:BaseQuantity>
        </cac:Price>
    </cac:InvoiceLine>

    <!-- ========================================
         INVOICE LINE 3: Order PO-2026-002, Delivery to Marseille Store
         ======================================== -->
    <cac:InvoiceLine>
        <!-- BT-126 Invoice line identifier -->
        <cbc:ID>3</cbc:ID>
        <!-- BT-132 Invoice line note -->
        <cbc:Note>Livraison magasin Marseille Vieux-Port</cbc:Note>
        <!-- BT-129 Invoiced quantity -->
        <cbc:InvoicedQuantity unitCode="EA">30</cbc:InvoicedQuantity>
        <!-- BT-131 Invoice line net amount -->
        <cbc:LineExtensionAmount currencyID="EUR">750.00</cbc:LineExtensionAmount>

        <!-- EXT-FR-FE-135: Order reference at line level (same order as line 2, different delivery) -->
        <cac:OrderLineReference>
            <cbc:LineID>12</cbc:LineID>
            <cac:OrderReference>
                <!-- This line also references PO-2026-002, but delivers to different location -->
                <cbc:ID>PO-2026-002</cbc:ID>
                <cbc:IssueDate>2026-01-08</cbc:IssueDate>
            </cac:OrderReference>
        </cac:OrderLineReference>

        <!-- EXT-FR-FE-BG-07: Despatch advice reference at line level -->
        <cac:DespatchLineReference>
            <!-- EXT-FR-FE-141: Despatch advice line reference -->
            <cbc:LineID>5</cbc:LineID>
            <cac:DocumentReference>
                <!-- EXT-FR-FE-140: Same despatch advice as line 2, different line -->
                <cbc:ID>DN-2026-0046</cbc:ID>
