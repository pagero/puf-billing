<?xml version="1.0" encoding="UTF-8"?>
<!--
    PUF France Use Case 1: Multi-Order / Multi-Delivery Invoice
    
    Business Scenario:
    - Consolidated invoice covering multiple purchase orders
    - Multiple delivery locations across different invoice lines
    - Demonstrates French extensions (EXTENDED-CTC-FR profile)
    
    Key Extensions Used:
    - EXT-FR-FE-135: Order reference at line level
    - EXT-FR-FE-140/141: Despatch advice reference at line level
    - EXT-FR-FE-BG-10: Delivery information group at line level
    - EXT-FR-FE-146/148/149: Location identifiers at line level
    - EXT-FR-FE-150: Delivery address at line level
    
    French Text Codes (BT-21/BT-22):
    - #REG#: Company legal form and share capital
    - #ABL#: Trade register number (RCS)
    - #AAI#: APE business activity code
    - #PMD#: Late payment penalties
    - #PMT#: Fixed collection costs indemnity (€40)
    - #BAR#: Treatment type (B2B e-invoicing)
    
    Example Pattern:
    - Invoice Line 1: Order PO-2026-001 → Warehouse Lyon
    - Invoice Line 2: Order PO-2026-002 → Store Paris Centre
    - Invoice Line 3: Order PO-2026-002 → Store Marseille
-->
<Invoice xmlns="urn:pagero:PageroUniversalFormat:Invoice:1.0" xmlns:cac="urn:pagero:CommonAggregateComponents:1.0" xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:puf="urn:pagero:ExtensionComponent:1.0" xmlns:ext="urn:oasis:names:specification:ubl:schema:xsd:CommonExtensionComponents-2">

    <!-- BT-24 Specification identifier -->
    <cbc:CustomizationID>urn:pagero.com:puf:billing:2.0</cbc:CustomizationID>
    <!-- BT-23 Business process type - B1: Goods invoice -->
    <cbc:ProfileID>urn:pagero.com:puf:billing:1.0</cbc:ProfileID>
    <!-- BT-1 Invoice Number -->
    <cbc:ID>INV-FR-2026-0158</cbc:ID>
    <!-- BT-2 Invoice Issue Date -->
    <cbc:IssueDate>2026-01-15</cbc:IssueDate>
    <!-- BT-9 Payment Due Date -->
    <cbc:DueDate>2026-02-14</cbc:DueDate>
    <!-- BT-3 Invoice Type Code - 380: Commercial invoice -->
    <cbc:InvoiceTypeCode name="B1">380</cbc:InvoiceTypeCode>

    <!-- BT-21/BT-22: Invoice notes with French text codes -->
    <!-- REG: Company legal form and share capital (BT-33 alternative method) -->
    <cbc:Note>#REG#SAS au capital de 500000.00 EUR</cbc:Note>
    <!-- ABL: Legal information - Trade register (BT-33 alternative method) -->
    <cbc:Note>#ABL#RCS Paris B 123 456 789</cbc:Note>
    <!-- AAI: General information - APE code (business activity classification) -->
    <cbc:Note>#AAI#APE 4690Z - Commerce de gros</cbc:Note>
    <!-- PMD: Late payment penalties (French mandatory payment terms) -->
    <cbc:Note>#PMD#En cas de retard de paiement, application des pénalités à un taux égal au taux d'intérêt appliqué par la BCE majoré de 10 points de pourcentage</cbc:Note>
    <!-- PMT: Fixed indemnity for collection costs -->
    <cbc:Note>#PMT#Indemnité forfaitaire pour frais de recouvrement en cas de retard de paiement : 40 EUR</cbc:Note>
    <!-- BAR: Treatment type qualification - B2B e-invoicing -->
    <cbc:Note>#BAR#B2B</cbc:Note>

    <!-- BT-5 Invoice Document Currency -->
    <cbc:DocumentCurrencyCode>EUR</cbc:DocumentCurrencyCode>

    <!-- BT-8 Tax point date code - 35: Delivery date -->
    <cac:InvoicePeriod>
        <cbc:DescriptionCode>35</cbc:DescriptionCode>
    </cac:InvoicePeriod>

    <!-- Header-level order references (BT-13): Lists all orders consolidated in this invoice -->
    <cac:OrderReference>
        <!-- BT-13 Purchase Order Reference - First order -->
        <cbc:ID>PO-2026-001</cbc:ID>
        <cbc:IssueDate>2026-01-05</cbc:IssueDate>
    </cac:OrderReference>

    <!-- Additional order reference using AdditionalDocumentReference for second order -->
    <cac:AdditionalDocumentReference>
        <cbc:ID>PO-2026-002</cbc:ID>
        <cbc:IssueDate>2026-01-08</cbc:IssueDate>
        <cbc:DocumentTypeCode>130</cbc:DocumentTypeCode>
        <cbc:DocumentDescription>Consolidated purchase order reference</cbc:DocumentDescription>
    </cac:AdditionalDocumentReference>

    <!-- BG-4 Seller Information -->
    <cac:AccountingSupplierParty>
        <cac:Party>
            <!-- BT-29 SIRET identifier -->
            <cac:PartyIdentification>
                <cbc:ID schemeID="0009">12345678901234</cbc:ID>
            </cac:PartyIdentification>
            <!-- BT-28 Seller Trading Name -->
            <cac:PartyName>
                <cbc:Name>Distributeur France Alimentaire</cbc:Name>
            </cac:PartyName>
            <!-- BG-5 Seller Address -->
            <cac:PostalAddress>
                <!-- BT-35 Street name -->
                <cbc:StreetName>25 Avenue des Champs-Élysées</cbc:StreetName>
                <cbc:AdditionalStreetName>Bâtiment A, 3ème étage</cbc:AdditionalStreetName>
                <!-- BT-37 City -->
                <cbc:CityName>Paris</cbc:CityName>
                <!-- BT-38 Postal code -->
                <cbc:PostalZone>75008</cbc:PostalZone>
                <cac:Country>
                    <!-- BT-40 Country code -->
                    <cbc:IdentificationCode>FR</cbc:IdentificationCode>
                    <cbc:Name>France</cbc:Name>
                </cac:Country>
            </cac:PostalAddress>
            <!-- BT-31 Seller VAT Number -->
            <cac:PartyTaxScheme>
                <cbc:CompanyID>FR12345678901</cbc:CompanyID>
                <cac:TaxScheme>
                    <cbc:ID>VAT</cbc:ID>
                </cac:TaxScheme>
            </cac:PartyTaxScheme>
            <cac:PartyLegalEntity>
                <!-- BT-27 Seller Legal Name -->
                <cbc:RegistrationName>Distributeur France Alimentaire SAS</cbc:RegistrationName>
                <!-- BT-30 SIREN -->
                <cbc:CompanyID schemeID="0002">123456789</cbc:CompanyID>
            </cac:PartyLegalEntity>
            <!-- BG-6 Seller Contact -->
            <cac:Contact>
                <!-- BT-41 Contact name -->
                <cbc:Name>Marie Dubois</cbc:Name>
                <!-- BT-42 Contact telephone -->
                <cbc:Telephone>+33 1 23 45 67 89</cbc:Telephone>
                <!-- BT-43 Contact email -->
                <cbc:ElectronicMail>factures@distributeur-france.fr</cbc:ElectronicMail>
            </cac:Contact>
        </cac:Party>
    </cac:AccountingSupplierParty>

    <!-- BG-7 Buyer Information -->
    <cac:AccountingCustomerParty>
        <cbc:SupplierAssignedAccountID>CUST-FR-98765</cbc:SupplierAssignedAccountID>
        <cac:Party>
            <!-- BT-46 Buyer Identifier - SIRET -->
            <cbc:EndpointID schemeID="0009">98765432109876</cbc:EndpointID>
            <!-- BT-45 Buyer Trading Name -->
            <cac:PartyName>
                <cbc:Name>Chaîne Magasins Retail France</cbc:Name>
            </cac:PartyName>
            <!-- BG-8 Buyer Address -->
            <cac:PostalAddress>
                <!-- BT-50 Street name -->
                <cbc:StreetName>150 Boulevard Haussmann</cbc:StreetName>
                <!-- BT-52 City -->
                <cbc:CityName>Paris</cbc:CityName>
                <!-- BT-53 Postal code -->
                <cbc:PostalZone>75009</cbc:PostalZone>
                <cac:Country>
                    <!-- BT-55 Country code -->
                    <cbc:IdentificationCode>FR</cbc:IdentificationCode>
                    <cbc:Name>France</cbc:Name>
                </cac:Country>
