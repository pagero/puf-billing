            <!-- EXT-FR-FE-158: Actual delivery date at line level -->
            <cbc:ActualDeliveryDate>2026-01-14</cbc:ActualDeliveryDate>
            <!-- Delivery location details -->
            <cac:DeliveryLocation>
                <!-- EXT-FR-FE-146: Location identifier (GLN for Marseille store) -->
                <cbc:ID schemeID="0088">3597890345678</cbc:ID>
                <!-- EXT-FR-FE-149: Location name -->
