                <!-- EXT-FR-FE-150: Delivery address at line level (Marseille store address) -->
                <cac:Address>
                    <!-- EXT-FR-FE-151: Street name -->
                    <cbc:StreetName>7 Quai du Port</cbc:StreetName>
                    <!-- EXT-FR-FE-154: City -->
                    <cbc:CityName>Marseille</cbc:CityName>
                    <!-- EXT-FR-FE-155: Postal code -->
                    <cbc:PostalZone>13002</cbc:PostalZone>
                    <!-- EXT-FR-FE-156: Country subdivision -->
                    <cbc:CountrySubentity>Provence-Alpes-Côte d'Azur</cbc:CountrySubentity>
                    <cac:Country>
                        <!-- EXT-FR-FE-157: Country code -->
                        <cbc:IdentificationCode>FR</cbc:IdentificationCode>
