<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Invoice xmlns:cac="urn:pagero:CommonAggregateComponents:1.0"
    xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2"
    xmlns="urn:pagero:PageroUniversalFormat:Invoice:1.0" xmlns:puf="urn:pagero:ExtensionComponent:1.0" xmlns:ext="urn:oasis:names:specification:ubl:schema:xsd:CommonExtensionComponents-2">
    

    <!-- Values in invoicereport are examples values only. Example of a purchase Bi2B cross border invoice being reported. Buyer in France. Seller in Germany -->
    
    <ext:UBLExtensions>
        <ext:UBLExtension>
            <ext:ExtensionURI>urn:pagero:ExtensionComponent:1.0:PageroExtension:RestrictedInformation</ext:ExtensionURI>
            <ext:ExtensionContent>
                <puf:PageroExtension>
                    <!-- Indication of type of taxreporting (can be INVOICE or RECEIPTTRANSACTION) -->
                    <puf:RestrictedInformation>
                        <puf:Key>entryType</puf:Key>
                        <puf:Value>INVOICE</puf:Value>
                    </puf:RestrictedInformation>
                    <!-- if a issuer wants to send a issuerAssignedReportId -->
                    <puf:RestrictedInformation>
                        <puf:Key>issuerAssignedReportId</puf:Key>
                        <puf:Value>Issuer assigned report id</puf:Value>
                    </puf:RestrictedInformation>
                    <!-- if a report period needs to be sent -->
                    <puf:RestrictedInformation>
                        <puf:Key>reportPeriodStart</puf:Key>
                        <puf:Value>2025-02-01</puf:Value>
                    </puf:RestrictedInformation>
                    <puf:RestrictedInformation>
                        <puf:Key>reportPeriodEnd</puf:Key>
                        <puf:Value>2025-02-28</puf:Value>
                    </puf:RestrictedInformation>
                </puf:PageroExtension>
            </ext:ExtensionContent>
        </ext:UBLExtension>
    </ext:UBLExtensions>

    <!-- BT-24 Specification identifier -->
    <cbc:CustomizationID>urn:pagero.com:puf:billing:2.0</cbc:CustomizationID>
    <!-- BT-23 Business process type -->
    <cbc:ProfileID>urn:pagero.com:puf:purchase:1.0</cbc:ProfileID>
    <!-- BT-1 Invoice Number -->
    <cbc:ID>INV123456</cbc:ID>
    <!-- BT-2 Invoice Issue Date  -->
    <cbc:IssueDate>2026-01-01</cbc:IssueDate>
    <!-- BT-9 Due Date -->
    <cbc:DueDate>2026-01-31</cbc:DueDate>
    <!-- BT-3 Invoice Type Code, in attribute @name the business process. -->
    <cbc:InvoiceTypeCode name="B1">380</cbc:InvoiceTypeCode>
    <!-- BT-22 - Invoice note. BT-21 note type between ## -->
    <cbc:Note>#AAA#Module électronique programmable pour applications technologiques avancées, doté de microprocesseurs haute performance, d’une mémoire embarquée de 16 Go, et d’interfaces multiples (USB, Ethernet, et GPIO). Ce composant est conçu pour le développement d’applications IoT, d’intelligence artificielle et de systèmes embarqués. Poids net : 250 g, dimensions : 12 x 8 x 2 cm. Usage : intégration dans des dispositifs électroniques industriels ou grand public. Produit non dangereux, conforme aux normes CE et RoHS.</cbc:Note>
    <!-- BT-5 Invoice Document Currency -->
    <cbc:DocumentCurrencyCode>EUR</cbc:DocumentCurrencyCode>
    <!-- BG-14 Invoicing Period -->
    <cac:InvoicePeriod>
        <!-- BT-73 - Invoicing period start date -->
        <cbc:StartDate>2025-12-01</cbc:StartDate>
        <!-- BT-74 - Invoicing period end date -->
        <cbc:EndDate>2026-01-31</cbc:EndDate>
        <!-- BT-8 - TAX point date code -->
        <cbc:Description>3</cbc:Description>
    </cac:InvoicePeriod>
    <!-- BG-4 Seller -->
    <cac:AccountingSupplierParty>
        <cac:Party>
            <cac:PostalAddress>
                <cac:Country>
                    <!-- BT-40 Seller address country code -->
                    <cbc:IdentificationCode>DE</cbc:IdentificationCode>
                </cac:Country>
            </cac:PostalAddress>
            <cac:PartyTaxScheme>
                <!-- BT-31 Seller TAX Number -->
                <cbc:CompanyID>DE00123123123</cbc:CompanyID>
                <cac:TaxScheme>
                    <cbc:ID>VAT</cbc:ID>
                </cac:TaxScheme>
            </cac:PartyTaxScheme>
            <cac:PartyLegalEntity>
                <!-- BT-27 Seller Name -->
                <cbc:RegistrationName>Seller company Name</cbc:RegistrationName>
                <!-- BT-30 Seller legal registration identifier -->
                <cbc:CompanyID schemeID="0223">123123123</cbc:CompanyID>
            </cac:PartyLegalEntity>
        </cac:Party>
    </cac:AccountingSupplierParty>
    <!-- BG-7 Buyer -->
    <cac:AccountingCustomerParty>
        <cac:Party>
            <cac:PostalAddress>
                <cac:Country>
                    <!-- BT-55 Buyer address country code -->
                    <cbc:IdentificationCode>FR</cbc:IdentificationCode>
                </cac:Country>
            </cac:PostalAddress>
            <cac:PartyTaxScheme>
                <!-- BT-48 Buyer TAX Number -->
                <cbc:CompanyID>FR00323456789</cbc:CompanyID>
                <cac:TaxScheme>
                    <cbc:ID>VAT</cbc:ID>
                </cac:TaxScheme>
            </cac:PartyTaxScheme>
            <cac:PartyLegalEntity>
                <!-- BT-44 Buyer Name -->
                <cbc:RegistrationName>Buyer Company Name</cbc:RegistrationName>
                <!-- BT-47 Buyer legal registration identifier. -->
                <cbc:CompanyID schemeID="0002">323456789</cbc:CompanyID>
            </cac:PartyLegalEntity>
        </cac:Party>
    </cac:AccountingCustomerParty>
    <cac:Delivery>
        <!-- BT-72 Delivery date-->
        <cbc:ActualDeliveryDate>2026-01-01</cbc:ActualDeliveryDate>
        <!-- BG-15 Delivery location/deliver to address -->
        <cac:DeliveryLocation>
            <cac:Address>
                <!-- BT-75 Deliver to street name-->
                <cbc:StreetName>ZAC du Cornillon Nord</cbc:StreetName>
                <!-- BT-77 Deliver to city -->
                <cbc:CityName>Saint-Denis</cbc:CityName>
                <!-- BT-78 Deliver to post code-->
                <cbc:PostalZone>93216</cbc:PostalZone>
                <cac:Country>
                    <!-- BT-80 Deliver to post code -->
                    <cbc:IdentificationCode>FR</cbc:IdentificationCode>
                </cac:Country>
            </cac:Address>
        </cac:DeliveryLocation>
    </cac:Delivery>
    <cac:TaxTotal>
        <!-- BT-110 Invoice total TAX amount. Currency in attribute currencyID (BT-5) -->
        <cbc:TaxAmount currencyID="EUR">400.00</cbc:TaxAmount>
        <cac:TaxSubtotal>
            <!-- BT-116 Sum of all taxable amounts subject to a specific TAX category code and TAX category rate. Currency in attribute currencyID (BT-5) -->
            <cbc:TaxableAmount currencyID="EUR">2000.00</cbc:TaxableAmount>
            <!--  BT-117. The total TAX amount for a given TAX category. Currency in attribute currencyID (BT-5) -->
            <cbc:TaxAmount currencyID="EUR">400.00</cbc:TaxAmount>
            <cac:TaxCategory>
                <!-- BT-118 TAX category code. -->
                <cbc:ID>S</cbc:ID>
                <!-- BT-119 TAX category rate-->
                <cbc:Percent>20</cbc:Percent>
                <cac:TaxScheme>
                    <cbc:ID>VAT</cbc:ID>
                </cac:TaxScheme>
            </cac:TaxCategory>
        </cac:TaxSubtotal>
    </cac:TaxTotal>
    <cac:LegalMonetaryTotal>
        <!-- BT-109 Invoice total amount without TAX. Currency in attribute currencyID (BT-5)-->
        <cbc:TaxExclusiveAmount currencyID="EUR">2000.00</cbc:TaxExclusiveAmount>
       <!-- BT-115 Amount due for payment. Currency in attribute currencyID (BT-5) -->
        <cbc:PayableAmount currencyID="EUR">2000.00</cbc:PayableAmount>
    </cac:LegalMonetaryTotal>
    <cac:InvoiceLine>
        <!-- BT-126 Line ID -->
        <cbc:ID>1</cbc:ID>
        <!-- BT-129 Invoiced Quantity. BT-130 unit of measure in attribute unitCode -->
        <cbc:InvoicedQuantity unitCode="NAR">100</cbc:InvoicedQuantity>
        <!-- BT-131 Line extension amount. Line net amount. Currency in attribute currencyID (BT-5) -->
        <cbc:LineExtensionAmount currencyID="EUR">2000.00</cbc:LineExtensionAmount>
        <cac:Item>
            <!-- BT-153 Item name -->
            <cbc:Name>Livraison de modules</cbc:Name>
            <!-- BG-30 LINE TAX INFORMATION -->
            <cac:ClassifiedTaxCategory>
                <!-- BT-151 Invoiced item TAX category code -->
                <cbc:ID>S</cbc:ID>
                <!-- BT-152 Invoiced item TAX rate -->
                <cbc:Percent>20</cbc:Percent>
                <cac:TaxScheme>
                    <cbc:ID>VAT</cbc:ID>
                </cac:TaxScheme>
            </cac:ClassifiedTaxCategory>
        </cac:Item>
        <!-- BG-29 PRICE DETAILS -->
        <cac:Price>
            <!-- BT-146 Item net price. Currency in attribute currencyID (BT-5) -->
            <cbc:PriceAmount currencyID="EUR">20.00</cbc:PriceAmount>
        </cac:Price>
    </cac:InvoiceLine>
</Invoice>
